{
  "label": "$SERVICE_NAME Deployment Policy",
  "description": "Horizon Deployment Policy for operator.ansible.nginx",
  "service": {
    "name": "$SERVICE_NAME",
    "org": "$HZN_ORG_ID",
    "arch": "*",
    "serviceVersions": [
      {
        "version": "$SERVICE_VERSION",
        "priority": {
          "priority_value": 1,
          "retries": 2,
          "retry_durations": 60
        }
      }
    ]
  },
  "properties": [
    {
      "name": "app",
      "value": "demo"
    }
  ],
  "constraints": ["app == demo"]
}
