{
    "label": "$SERVICE_NAME Deployment Policy",
    "description": "Horizon Deployment Policy for operator.ansible.nginx",
    "service": {
        "name": "$SERVICE_NAME",
        "org": "$HZN_ORG_ID",
        "arch": "*",
        "serviceVersions": [
        {
            "version": "$SERVICE_VERSION",
            "priority":{}
        }
        ]
    },
    "properties": [
        {
            "name": "sampleapp",
            "value": "$SERVICE_VERSION"
        }
    ],
    "constraints": [
        "sampleapp == $SERVICE_VERSION"
    ]
}